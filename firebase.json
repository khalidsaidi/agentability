{
  "functions": [
    {
      "source": "apps/functions",
      "codebase": "default",
      "runtime": "nodejs20",
      "predeploy": [
        "pnpm -C apps/web build",
        "pnpm build:ssr-assets",
        "pnpm -C apps/functions build"
      ]
    }
  ],
  "hosting": {
    "public": "apps/web/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "predeploy": [
      "pnpm build:artifacts",
      "pnpm -C apps/web build"
    ],
    "headers": [
      {
        "source": "/.well-known/*.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json; charset=utf-8"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          },
          {
            "key": "X-Agentability-Spec-Version",
            "value": "1.0"
          },
          {
            "key": "X-Agentability-Build",
            "value": "ebd2a83"
          }
        ]
      },
      {
        "source": "/.well-known/*.yaml",
        "headers": [
